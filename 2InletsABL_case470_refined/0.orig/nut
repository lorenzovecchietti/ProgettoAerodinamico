FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];
internalField   uniform 5e-07;

boundaryField
{
    inlet_water
    {
        type            fixedValue;
        value           $internalField;
    }

    inlet_air
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    atmosphere
    {
        type            zeroGradient;
    }
    
    side
    {
        type            symmetry;
    }

    bottom
    {
        type            symmetryPlane;
    }
}


